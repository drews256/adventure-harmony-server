# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Database Configuration for Agno Session Storage
# Note: If password contains special characters, they must be URL-encoded
# For example: & becomes %26, @ becomes %40, etc.
# Use postgresql+psycopg:// prefix for Agno PostgresStorage
# Example for Supabase pooler: postgresql+psycopg://postgres.project_id:password@aws-0-region.pooler.supabase.com:5432/postgres
DATABASE_URL=postgresql+psycopg://user:password@host:port/database

# Anthropic API Key
ANTHROPIC_API_KEY=your_anthropic_api_key

# MCP Server Configuration
MCP_SERVER_URL=https://goguide-mcp-server-b0a0c27ffa32.herokuapp.com

# Server Configuration
PORT=3000

# Worker Configuration (optional)
POLL_INTERVAL=5

# A2A Configuration (optional)
A2A_AGENT_ID=message-analysis-agent
A2A_AGENT_NAME=Message Analysis Agent