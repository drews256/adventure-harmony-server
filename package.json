{
  "name": "message-analysis-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:worker": "node dist/worker-entry.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:worker\"",
    "dev:server": "ts-node-dev --respawn src/server.ts",
    "dev:worker": "ts-node-dev --respawn src/worker-entry.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest tests/e2e",
    "test:integration": "jest tests/integration",
    "test:unit": "jest tests/services"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.50.2",
    "@modelcontextprotocol/sdk": "^1.11.2",
    "@supabase/supabase-js": "^2.7.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.100",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
